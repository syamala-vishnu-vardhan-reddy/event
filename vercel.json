{
  "version": 2,
  "framework": "vite",
  "rootDirectory": "frontend",
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }

  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
